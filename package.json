{
    "name": "win_local_dns",
    "version": "1.0.0",
    "description": "",
    "isApp": false,
    "author": "Yuri Yaryshev",
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "bin": {
        "updateddns": "./src/cmd/updateddns.cjs"
    },
    "exports": {
        ".": {
            "import": "./lib/esm/index.js",
            "require": "./lib/cjs/index.js"
        }
    },
    "typings": "lib/types/index.d.ts",
    "homepage": "https://github.com/yuyaryshev/win_local_dns",
    "repository": {
        "type": "git",
        "url": "https://github.com/yuyaryshev/win_local_dns"
    },
    "license": "Unlicense",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "precompile": "yb precompile && prettier src --write",
        "precompile_full": "npm run precompile && prettier src --write",
        "clean:cjs": "yb clean_cjs",
        "build:cjs": "npm run clean:cjs && title win_local_dns - watch:cjs & babel src --config-file ./babel.cjs.config.cjs --out-dir lib/cjs --extensions \".ts,.tsx,.js,.jsx\" --source-maps",
        "watch:cjs": "npm run clean:cjs && title win_local_dns - watch:cjs & babel src --config-file ./babel.cjs.config.cjs --out-dir lib/cjs --extensions \".ts,.tsx,.js,.jsx\" --source-maps -w",
        "clean:esm": "yb clean_esm",
        "build:esm": "npm run clean:esm && babel src --config-file ./babel.esm.config.cjs --out-dir lib/esm --extensions \".ts,.tsx,.js,.jsx\" --source-maps && node mjs_import.test.mjs && echo mjs import is ok!",
        "watch:esm": "npm run clean:esm && babel src --config-file ./babel.esm.config.cjs --out-dir lib/esm --extensions \".ts,.tsx,.js,.jsx\" --source-maps -w",
        "clean:types": "yb clean_types",
        "build:types": "npm run clean:types && tsc -p tsconfig-declarations.json",
        "watch:types": "npm run clean:types && title win_local_dns - watch:types & tsc -p tsconfig-declarations.json -w",
        "test": "mocha './lib/cjs/**/*.test.js'",
        "installglobally": "npm -g ."
    },
    "resolutions": {
        "yhttp_api": "^1.0.13",
        "yhttp_api_express": "^1.0.13",
        "@types/node": "^20.11.17"
    },
    "devDependencies": {
        "@babel/cli": "7.24.1",
        "@babel/core": "7.24.4",
        "@babel/node": "7.23.9",
        "@babel/parser": "7.24.4",
        "@babel/plugin-proposal-decorators": "7.24.1",
        "@babel/plugin-transform-modules-commonjs": "^7.24.1",
        "@babel/plugin-transform-typescript": "7.24.4",
        "@babel/preset-react": "^7.24.1",
        "@babel/preset-typescript": "^7.24.1",
        "@types/babel__core": "^7.20.5",
        "@types/chai": "4.3.14",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.12.7",
        "@types/react": "^18.2.75",
        "@types/react-dom": "^18.2.24",
        "@types/sinon": "17.0.3",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/ui": "^1.4.0",
        "babel-plugin-inline-replace-variables": "^1.3.1",
        "babel-plugin-module-resolver": "^5.0.0",
        "babel-watch": "^7.8.1",
        "cross-env": "^7.0.3",
        "javascript-stringify": "^2.1.0",
        "madge": "^7.0.0",
        "mocha": "^10.4.0",
        "modify-filepath": "*",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "sinon": "^17.0.1",
        "source-map-support": "^0.5.21",
        "tslib": "^2.6.2",
        "typescript": "5.4.5",
        "vite": "^5.2.8",
        "yyabuilder": "^4.3.2"
    },
    "dependencies": {
        "@types/better-sqlite3": "^7.6.9",
        "@types/express": "^4.17.21",
        "@types/fs-extra": "^11.0.4",
        "@types/luxon": "3.4.2",
        "chokidar": "^3.6.0",
        "fs-extra": "^11.2.0",
        "json5": "^2.2.3",
        "minio": "^7.1.3",
        "ystd": "^3.11.0",
        "ystd_server": "^3.8.0",
        "yuyaryshev-json-type-validation": "^3.1.8"
    }
}
